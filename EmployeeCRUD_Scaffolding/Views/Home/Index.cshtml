@model IEnumerable<EmployeeCRUD_Scaffolding.Employee>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.EmployeeName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EmployeeSalary)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EmployeeCIty)
        </th>
        <th></th>
    </tr>

@foreach (Employee emp in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => emp.EmployeeName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => emp.EmployeeSalary)
        </td>
        <td>
            @Html.DisplayFor(modelItem => emp.EmployeeCIty)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=emp.EmployeeId }) |
            @Html.ActionLink("Details", "Details", new { id=emp.EmployeeId }) |
            @Html.ActionLink("Delete", "Delete", new { id=emp.EmployeeId })
        </td>
    </tr>
}

</table>
